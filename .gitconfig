[user]
	name = Michael Brandt
	email = michaelbrandt5@gmail.com
[color]
	ui = true
[core]
	pager = less --tabs=2
	excludesfile = ~/.gitignore
	editor = emacsclient -nw
[push]
	default = simple
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[help]
	autocorrect = 1
[rebase]
	autosquash = true
[diff]
	compactionHeuristic = true
[interactive]
	diffFilter = diff-highlight
[magit]
	hideCampaign = true
